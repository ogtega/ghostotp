ard Library JDK 8 extension\n     */\n    implementation Dependencies.kotlin_stdlib\n    /**\n     * Support Library Packages\n     */\n    implementation Dependencies.appcompat\n    implementation Dependencies.constraint_layout\n    implementation Dependencies.material\n    /**\n     * Lifecycle Components (Lifecycle, LiveData & ViewModel)\n     */\n    implementation Dependencies.lifecycle\n    implementation Dependencies.lifecycle_viewmodel\n    /**\n     * Room Persistence Library\n     */\n    kapt Annotations.room\n    implementation Dependencies.room\n    implementation Dependencies.room_runtime\n    /**\n     * Navigation Components\n     */\n    implementation Dependencies.navigation_fragment\n    implementation Dependencies.navigation_ui\n    /**\n     * Barcode Library\n     */\n    implementation Dependencies.camerax_core\n    implementation Dependencies.camerax_lifecycle\n    implementation Dependencies.camerax_view\n    implementation Dependencies.mlkit_barcode\n    /**\n     * Miscellaneous Libraries\n     */\n    implementation Dependencies.androidx\n    implementation Dependencies.fragment\n    implementation Dependencies.preference\n    implementation Dependencies.work\n    implementation Dependencies.apache_codec\n    implementation Dependencies.biometric\n    /**\n     * Test Dependencies\n     */\n    testImplementation Testing.junit\n    testImplementation Testing.room\n\n    androidTestImplementation Testing.android_junit\n    androidTestImplementation Testing.android_espresso\n    androidTestImplementation Testing.android_work\n}\n
